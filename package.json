{
	"name": "tonygl",
	"version": "0.0.3",
	"engines": {
		"vscode": ">=0.9.0-pre.1"
	},

	"displayName": "Tonygl",
	"publisher": "gamekiller2048",
	"description": "Syntax Highlighting and Support for Tonygl",
	"homepage": "./README.md",

	"icon": "./icons/logo.png",
	"repository": {
		"type": "git",
		"url": "git://github.com/gamekiller2048/tonygl-extension.git"
	},

	"contributes": {
		"languages": [
			{
				"id": "tonygl",
				"aliases": ["Tonygl", "tonygl"],
				"extensions": [".tonygl", ".tg"],
				"configuration": "./language-configuration.json",
				"icon": {
					"light": "./icons/logo.png",
					"dark": "./icons/logo.png"
				}
			}
		],
		"grammars": [
			{
				"language": "tonygl",
				"scopeName": "source.tg",
				"path": "./syntaxes/tonygl.tmLanguage.json"
			}
		],
		"themes": [
			{
				"label": "Tonygl Dark Theme",
				"uiTheme": "vs-dark",
				"path": "./themes/tonygl.tmTheme.json"
			},
			{
				"label": "Tonygl Light Theme",
				"uiTheme": "hc-light",
				"path": "./themes/tonygl.tmTheme.json"
			}
		]
	}
}